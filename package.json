{
  "name": "react-starter",
  "version": "2.0.0",
  "description": "My personal react starter project",
  "main": "index.js",
  "repository": "https://github.com/tysmithnet/react-starter",
  "author": "@tysmithnet",
  "license": "GPL-3.0-or-later",
  "private": false,
  "scripts": {
    "build": "webpack --color --mode production --config webpack.config.prd.js",
    "dep": "arkit",
    "doc": "typedoc --tsconfig tsconfig.json --out dist/docs/ --ignoreCompilerErrors src/ && http-server dist/docs",
    "e2e": "cypress open",
    "lint": "eslint src/**/*.ts?",
    "lint:fix": "eslint --fix src/**/*.ts?",
    "prebuild": "rimraf ./dist/*",
    "start": "webpack-dev-server --progress --color --mode development --config webpack.config.dev.js",
    "storybook": "start-storybook",
    "test": "jest --coverage"
  },
  "dependencies": {
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-redux": "7.1.3",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "redux-logger": "3.0.6",
    "redux-saga": "1.1.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "5.3.12",
    "@storybook/addon-info": "5.3.12",
    "@storybook/addon-knobs": "5.3.12",
    "@storybook/addon-notes": "5.3.12",
    "@storybook/react": "5.3.12",
    "@types/enzyme": "3.10.5",
    "@types/jest": "25.1.2",
    "@types/react": "16.9.19",
    "@types/react-dom": "16.9.5",
    "@typescript-eslint/eslint-plugin": "2.19.0",
    "@typescript-eslint/parser": "2.19.0",
    "arkit": "1.6.2",
    "babel-loader": "8.0.6",
    "babel-preset-react-app": "9.1.1",
    "css-loader": "3.4.2",
    "cypress": "4.0.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "fork-ts-checker-notifier-webpack-plugin": "2.0.0",
    "fork-ts-checker-webpack-plugin": "4.0.3",
    "html-webpack-plugin": "3.2.0",
    "http-server": "0.12.1",
    "jest": "25.1.0",
    "node-sass": "4.13.1",
    "prettier": "1.19.1",
    "react-docgen-typescript-loader": "3.6.0",
    "rimraf": "3.0.2",
    "sass-loader": "7.3.1",
    "source-map-loader": "0.2.4",
    "style-loader": "1.1.3",
    "ts-jest": "25.2.0",
    "ts-loader": "6.2.1",
    "tslib": "1.10.0",
    "typedoc": "0.16.9",
    "typescript": "3.7.5",
    "webpack": "4.6.0",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.3"
  }
}
